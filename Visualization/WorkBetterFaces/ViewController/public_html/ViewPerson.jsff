                                                </dvt:timelineItem>
                                            </dvt:timelineSeries>
                                            <dvt:timeAxis id="ta1" scale="weeks" zoomOrder="months"/>
                                            <dvt:timelineOverview id="ov1"
                                                                  inlineStyle="#{orientationManager.portraitMode ? 'width:60px' : 'height:60px'}">
                                                <dvt:timeAxis id="ta2" scale="months"/>
