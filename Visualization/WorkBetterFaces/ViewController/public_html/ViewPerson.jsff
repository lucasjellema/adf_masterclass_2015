                        <af:panelGroupLayout id="infd4" layout="vertical" styleClass="AFStretchWidth">
                            <af:decorativeBox id="db5">
                                <f:facet name="center">
                                    <dvt:hierarchyViewer id="hv1" var="node" controlPanelBehavior="hidden"
                                                         value="#{bindings.me1.treeModel}"
