                                                        <dvt:chartDataItem id="di11" value="#{row.salary}"
                                                                           series="#{bindings.salary.hints.label}"
                                                                           group="#{row.effective}" color="#68c182"/>
                                                        <dvt:chartDataItem id="di12" value="#{row.bonus}"
                                                                           series="#{bindings.bonus.hints.label}"
                                                                           group="#{row.effective}" color="#4a85c5"/>
                                                        <dvt:chartDataItem id="di13" value="#{row.commission}"
                                                                           series="#{bindings.commission.hints.label}"
                                                                           group="#{row.effective}" color="#F4AA46"/>
                                                    </af:group>
                                                </f:facet>
                                            </dvt:barChart>
                                            <af:spacer id="s11" width="1" height="25"/>
                                        </af:panelGroupLayout>
                                        <af:spacer width="50" height="30" id="s16"/>
