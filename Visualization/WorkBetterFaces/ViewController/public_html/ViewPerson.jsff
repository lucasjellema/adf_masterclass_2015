                                                            <dvt:attributeMatchRule id="amr1" group="Salary">
                                                                <f:attribute name="color" value="#68c182"/>
                                                            </dvt:attributeMatchRule>
                                                            <dvt:attributeMatchRule id="amr2" group="Bonus">
                                                                <f:attribute name="color" value="#4a85c5"/>
                                                            </dvt:attributeMatchRule>
                                                            <dvt:attributeMatchRule id="amr3" group="Commission">
                                                                <f:attribute name="color" value="#F4AA46"/>
                                                            </dvt:attributeMatchRule>
                                                        </dvt:attributeGroups>
                                                    </dvt:pieDataItem>
                                                </dvt:pieChart>
                                                <af:spacer height="15px" id="s18"/>
                                                <af:panelGridLayout id="pgl4" inlineStyle="width:250px;">
                                                    <af:gridRow marginTop="0px" height="auto" id="gr4">
                                                        <af:gridCell marginStart="5px" width="60%" id="gc5">
                                                            <af:panelGroupLayout layout="horizontal" id="pgl24"
                                                                                 halign="start">
                                                                <af:panelGroupLayout layout="vertical" id="s21"
                                                                                     styleClass="WBSalaryBar"/>
                                                                <af:spacer width="5px" id="s20"/>
                                                                <af:outputLabel id="ol5" value="Salary"
                                                                                styleClass="WBCardTitle"/>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                        <af:gridCell marginStart="5px" width="40%" marginEnd="5px"
                                                                     id="gc9" valign="middle" halign="end">
                                                            <af:outputText value="#{bindings.salary.inputValue}"
                                                                           styleClass="WBCompensationLegendSalary"
                                                                           id="ol15">
                                                                <af:convertNumber pattern="'$'#,###"/>
                                                            </af:outputText>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="0px" height="auto" id="gr5">
                                                        <af:gridCell marginStart="5px" width="60%" id="gc11">
                                                            <af:panelGroupLayout layout="horizontal" id="pgl22"
                                                                                 halign="start">
                                                                <af:panelGroupLayout layout="vertical" id="pgl27"
                                                                                     styleClass="WBBonusBar"/>
                                                                <af:spacer width="5px;" id="s22"/>
                                                                <af:outputLabel value="Bonus" styleClass="WBCardTitle"
                                                                                id="ol16"/>
                                                            </af:panelGroupLayout>
                                                        </af:gridCell>
                                                        <af:gridCell marginStart="5px" width="40%" marginEnd="5px"
                                                                     id="gc12" valign="middle" halign="end">
                                                            <af:outputText value="#{bindings.bonus.inputValue}"
                                                                           styleClass="WBCompensationLegendBonus"
                                                                           id="ol19">
                                                                <af:convertNumber pattern="'$'#,###"/>
                                                            </af:outputText>
                                                        </af:gridCell>
                                                    </af:gridRow>
                                                    <af:gridRow marginTop="0px" height="auto" marginBottom="5px"
                                                                id="gr6">
                                                        <af:gridCell marginStart="5px" width="60%" id="gc13">
                                                            <af:panelGroupLayout layout="horizontal" id="pgl23"
                                                                                 halign="start">
                                                                <af:panelGroupLayout layout="vertical" id="pgl28"
                                                                                     styleClass="WBCommissionBar"/>
                                                                <af:spacer width="5px" id="s24"/>
                                                                <af:outputLabel value="Commission"
                                                                                styleClass="WBCardTitle" id="ol18"/>
