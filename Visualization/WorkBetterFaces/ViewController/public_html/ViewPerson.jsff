                                                    <af:clientListener method="InfoTileComp.raiseSelectionEvent"
                                                                       type="click"/>
                                                </af:panelGridLayout>
                                            </af:panelGroupLayout>
                                        </af:panelGroupLayout>
                                        <!-- Compensation Ratio -->
                                    </af:panelGroupLayout>
                                </f:facet>
                            </af:decorativeBox>
