                                            <af:panelGroupLayout layout="vertical" halign="center"
                                                                 inlineStyle="width:450px" id="pgl26">
                                                <af:spacer width="50" id="s17"/>
                                                <dvt:pieChart id="pieChart2" value="#{bindings.pieComps.treeModel}"
                                                              var="chartRow" inlineStyle="width:200px;height:200px;"
                                                              sliceLabelPosition="none">
                                                    <af:transition transition="auto" triggerType="display">
                                                        <?audit suppress oracle.jdeveloper.jsp.check-valid-parent?>
                                                        <!--af:transition is supported at run time but flagging it as an error at design time is a known issue in 12.1.3-->
                                                    </af:transition>
                                                    <dvt:chartLegend id="cl2" rendered="false"/>
                                                    <dvt:pieDataItem label="#{chartRow.itemLabel}"
                                                                     value="#{chartRow.itemValue}" borderColor="#FFFFFF"
                                                                     id="pdi1">
